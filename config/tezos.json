{
    "root": "/tezos",
    "endpoint": "localhost:8732",
    "protocol": "http",
    "URLs": [
        {
	    "url": "/staking_balance/:head/:account",
            "source": "/chains/main/blocks/$head/context/delegates/$account/staking_balance",
	    "params": ["head", "account"]
	},
	{
	    "url": "/current_level/:head",
	    "source": "/chains/main/blocks/$head/helpers/current_level",
	    "params": ["head"]
	},
	{
	    "url": "/blockheader/:head",
	    "source": "/chains/main/blocks/$head/header",
	    "params": ["head"]
	},
	{
	    "url": "/metadata/:head",
	    "source": "/chains/main/blocks/$head/metadata",
	    "params": ["head"]
	},
	{
	    "url": "/cycle_info/:head/:cycle",
	    "source": "/chains/main/blocks/$head/context/raw/json/cycle/$cycle",
	    "params": ["head", "cycle"]
	},
	{
	    "url": "/endorsing_rights/:head/:account/:cycle",
	    "source": "/chains/main/blocks/$head/helpers/endorsing_rights?cycle=$cycle&delegate=$account",
	    "query": ["head", "cycle", "account"]
	},
	{
	    "url": "/baking_rights/:head/:account/:cycle",
	    "source": "/chains/main/blocks/$head/helpers/baking_rights?cycle=$cycle&delegate=$account",
	    "query": ["head", "cycle", "delegate"]
	},
	{
	    "url": "/operations/:head",
	    "source": "/chains/main/blocks/$head/operations",
	    "params": ["head"]
	},
	{
	    "url": "/delegated_contracts/:head/:account",
	    "source": "/chains/main/blocks/$head/context/delegates/$account/delegated_contracts",
	    "params": ["head", "account"]
	},
	{
	    "url": "/delegated_balance/:head/:account",
	    "source": "/chains/main/blocks/$head/context/delegates/$account/delegated_balance",
	    "params": ["head", "account"]
	},
        {
	    "url": "/delegate_balance/:head/:account",
            "source": "/chains/main/blocks/$head/context/delegates/$account/balance",
	    "params": ["head", "account"]
	},
        {
	    "url": "/frozen_balance/:head/:account",
            "source": "/chains/main/blocks/$head/context/delegates/$account/frozen_balance",
	    "params": ["head", "account"]
	},
        {
	    "url": "/frozen_balance_by_cycle/:head/:account",
            "source": "/chains/main/blocks/$head/context/delegates/$account/frozen_balance_by_cycle",
	    "params": ["head", "account"]
	},
        {
	    "url": "/balance/:head/:account",
            "source": "/chains/main/blocks/$head/context/contracts/$account/balance",
	    "params": ["head", "account"]
	},
        {
	    "url": "/counter/:contract",
            "source": "/chains/main/blocks/head/context/contracts/$contract/counter",
	    "params": ["contract"]
	},
        {
	    "url": "/manager_key/:contract",
            "source": "/chains/main/blocks/head/context/contracts/$contract/manager_key",
	    "params": ["contract"]
	},
        {
	    "url": "/block/:hash",
            "source": "/chains/main/blocks/$hash",
	    "params": ["hash"]
	},
        {
	    "url": "/blocks",
            "source": "/chains/main/blocks",
	    "params": []
	},
        {
	    "url": "/protocols",
            "source": "/chains/main/protocols",
	    "params": []
	}
    ]
}
