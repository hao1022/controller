{
    "root": "/eos",
    "host": "api.eosnewyork.io",
    "protocol": "http",
    "forwardings": [
        {
	    "source": "/get_info",
            "target": "/v1/chain/get_info"
	},
        {
	    "source": "/get_block_header_state/:block_num_or_id",
            "target": "/v1/chain/get_block_header_state",
	    "method": "post",
	    "data": "{\"block_num_or_id\":\"$block_num_or_id\"}",
	    "params": ["block_num_or_id"]
	}
    ]
}
